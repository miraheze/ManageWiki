{
	"root": true,
	"extends": [
		"wikimedia/client",
		"wikimedia/jquery",
		"wikimedia/mediawiki"
	],
	"ignorePatterns": [ "vendor/**" ],
	"rules": {
		"max-len": "off",
		"no-jquery/no-global-selector": "off",
		"no-jquery/no-sizzle": "off",
		"prefer-const": "off"
	},
	"parserOptions": {
		"ecmaVersion": 2024
	}
}
